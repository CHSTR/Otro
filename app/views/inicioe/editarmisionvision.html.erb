<main class="envoltura-16 espacio-vinf espacio-vsup ">
  <div class="row">
    <div class=" col-sm-12 col-md-3">
      <!-- DE ACA SE PUEDE PEGAR LAS CASILLAS -->
      <div class="container-fluid">
        <ol class="breadcrumb margen">
          <li class="breadcrumb-item" style="font-family: 'Junge', serif; font-size:15px;"><a href="../portal">Portal</a></li>
          <li class="breadcrumb-item active" style="font-family: 'Junge', serif; font-size:15px;">Escuela</li>
        </ol>
        <hr>
      </div>
      <div class="container-fluid margen" id="menu-escritorio">
        <div class="list-group">
          <a href="../inicioe/inicio" class="list-group-item list-group-item-action" style="font-family: 'Junge', serif;"><h4>Inicio</h4></a>
          <a href="../inicioe/historia" class="list-group-item list-group-item-action" style="font-family: 'Junge', serif;">
            <h4>Historia</h4>
          </a>
          <a href="../inicioe/misionvision" class="list-group-item list-group-item-action active" style="font-family: 'Junge', serif;"><h4>Misión y Visión</h4></a>
          <a href="../inicioe/pde" class="list-group-item list-group-item-action" style="font-family: 'Junge', serif;"><h4>PDE</h4></a>
        </div>
      </div>
      <!-- HASTA ACA -->
    </div>
    <div class=" col-sm-12 col-md-9 margen">
      <h1 style="font-family: 'Junge', serif;">Mision y Vision</h1>
      <div class="container margen text-justify" style="font-family: 'Junge', serif; font-size:18px;">
        <% if can? :manage, Etexto %>
        <%= form_for(@etextos, :url => {:action => :updatemisionvision}) do |f| %>
            <% if @etextos.errors.any? %>
                <div class="margen alert alert-danger alert-dismissible fade show" role="alert">
                  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                  </button>
                  <% @etextos.errors.full_messages.each do |msg| %>
                      <li><%= msg %></li>
                  <% end %>
                </div>
            <% end %>
            <div class="col-md-9 margen">
              <h3 class="text-left">
                <%= f.label :Texto %>
                <%= f.cktext_area :descripcion %>
              </h3>
              <br>
              <div class="actions">
                <%= f.submit 'Guardar Cambios', :class => 'btn btn-outline-success margen1' %>
              </div>
        <% end %>
            <% else %>
            <br>
            <div class="alert alert-danger alert-dismissible fade show" role="alert">
              <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
              <strong>No puede ver este contenido!</strong> debe iniciar sesion.
            </div>
            <br>
            <% end %>
        <br>
        <%= link_to 'Volver', inicioe_misionvision_path , :class => 'btn btn-outline-primary margen1' %>
        <br>
  </div>
  </div>
  <!-- Inicio Limpiar flotante -->
  <div class="limpiar"></div>
  <!-- Fin Limpiar flotante -->
</main>
<!-- FIN CONTENIDO PRINCIPAL -->

