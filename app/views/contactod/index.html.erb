<!-- Modal -->
<div class="modal fade bd-example-modal-lg" id="exampleModalLong" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle">Oferta Laboral</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <%= form_for(@oferta_laborals, :url => {:action => :crear}) do |f|%>
                <% if @oferta_laborals.errors.any? %>
                    <div class="margen alert alert-danger alert-dismissible fade show" role="alert">
                      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                      </button>
                      <% @oferta_laborals.errors.full_messages.each do |msg| %>
                      <li><%= msg %></li>
                      <% end %>
                    </div>
                <% end %> 
                <div class="form-group">
                    <label class="control-label margenl" for="Nombre">Nombres</label>
                    <br>
                  <%= f.text_field :nombre, :class => "form-control form-control-lg" %>
                </div>            
                <div class="form-group margen1" id="DOM">
                    <label class="control-label margenl" id="DOM" for="Empresa">Empresa</label>
                    <br>
                    <%= f.text_field :empresa, :class => "form-control form-control-lg" %>
                </div>
                <div class="form-group">
                <%= f.hidden_field :oferta_d, :value => 0 %>
                </div>
                <div class="form-group" id="DOM1">
                    <label class="control-label margenl" id="DOM" for="Empresa">Cargo</label>
                    <br>
                    <%= f.text_field :cargo, :class => "form-control form-control-lg" %>
                </div>
                <div class="form-group">
                    <label class="control-label" for="Correo">Dirección de Correo Electrónico</label>
                    <br>
                    <%= f.text_field :correo, :class => "form-control form-control-lg" %>
                </div>
                <div class="form-group">
                    <label class="control-label" for="Mensaje" id="p1">Mensaje</label>
                    <br>
                    <%= f.trix_editor :descripcion %>
                </div>
                    <%= f.hidden_field :aceptado, :value => '0'%>
                <div class="modal-footer">               
                    <input type="submit" class="btn btn-primary btn-lg" value="Enviar">
                    <input type="reset" class="btn btn-default btn-lg" value="Limpiar">                
                </div>
                <div id="respuesta" style="display: none;"></div>
              </form>
                <% end %> 
      </div> 
    </div>
  </div>
</div>

<div class="modal fade bd-example-modal-lg" id="exampleModalLong2" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
