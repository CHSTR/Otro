<main class="envoltura-16 espacio-vinf espacio-vsup ">
    <div class="row">
      <div class="">
        <!-- DE ACA SE PUEDE PEGAR LAS CASILLAS -->
        <!-- HASTA ACA -->
      </div>
      <div class="col-sm-12 col-md-12">
        <h4>Escuela de informática</h4>
            <div class="container">
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d699.7005120016598!2d-70.59784027403448!3d-33.466099071778224!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x9662cfc270900eb7%3A0x330e0d87329f4a5f!2sUniversidad+Tecnol%C3%B3gica+Metropolitana+-+Campus+Macul!5e0!3m2!1ses-419!2scl!4v1507604787839" style="border:0" allowfullscreen="" frameborder="0" height="250" width="100%"></iframe>
            </div>
            <strong>Contactenos</strong><br><strong>Oficina:</strong> ...
            <hr>
      </div>
      <div class="">
      </div>
      <div class=" col-sm-12 push-md-2 col-md-6 margen">
        <%= form_for(@oferta_laborals, :url => {:action => :crear}) do |f|%>
                <% if @oferta_laborals.errors.any? %>
                    <div class="margen alert alert-danger alert-dismissible fade show" role="alert">
                      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                      </button>
                      <% @oferta_laborals.errors.full_messages.each do |msg| %>
                      <li><%= msg %></li>
                      <% end %>
                    </div>
                <% end %> 
                <div class="form-group">
                    <label class="control-label margenl" for="Nombre">Nombres</label>
                    <br>
                  <%= f.text_field :nombre %>
                </div>            
                <label class="control-label" for="Motivo">Motivo de Contacto</label>
                <br>
                <%= f.radio_button :oferta_d, 0, id: "0", name: "oferta_d", onChange: "mostrar(0);" %> Consulta
                <%= f.radio_button :oferta_d, 1, id: 1, name: "oferta_d", onChange: "mostrar(1);", required: "required" %> Oferta Laboral
                <br>
                <div class="form-group margen1" id="DOM">
                    <label class="control-label margenl" id="DOM" for="Empresa">Empresa</label>
                    <br>
                    <%= f.text_field :empresa %>
                </div>
                <div class="form-group" id="DOM1">
                    <label class="control-label margenl" id="DOM" for="Empresa">Cargo</label>
                    <br>
                    <%= f.text_field :cargo %>
                </div>
                <div class="form-group">
                    <label class="control-label" for="Correo">Dirección de Correo Electrónico</label>
                    <br>
                    <%= f.text_field :correo %>
                </div>
                <div class="form-group">
                    <label class="control-label" for="Mensaje" id="p1">Mensaje</label>
                    <br>
                    <%= f.cktext_area :descripcion %>
                </div>
                    <%= f.hidden_field :aceptado, :value => '0'%>
                <div class="form-group">                
                    <input type="submit" class="btn btn-primary btn-lg" value="Enviar">
                    <input type="reset" class="btn btn-default btn-lg" value="Limpiar">                
                </div>
                <div id="respuesta" style="display: none;"></div>
            </form>
        <% end %>  
      </div>
    </div>
 <!-- Fin Noticias -->
 <!-- Inicio Agenda -->
 <!-- Fin Agenda -->
