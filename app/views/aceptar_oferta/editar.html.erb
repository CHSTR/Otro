<!-- INICIO CONTENIDO PRINCIPAL -->
<main class="envoltura-16 espacio-vinf espacio-vsup ">
    <div class="row">
      <div class=" col-sm-12 col-md-3">
        <!-- DE ACA SE PUEDE PEGAR LAS CASILLAS -->
        <div class="container-fluid">
          <ol class="breadcrumb margen">
            <li class="breadcrumb-item" style="font-family: 'Junge', serif; font-size:15px;"><a href=""><%= link_to 'Portal', portal_path %></a></li>
            <li class="breadcrumb-item active" style="font-family: 'Junge', serif; font-size:15px;">Departamento</li>
          </ol>
          <hr>
        </div>
      </div>
      <div class=" col-sm-12 col-md-9 margen">
      <div class="container-fluid">
        <% if can? :manage, OfertaLaboral %>
          <%= form_for(@oferta_laborals, :url => {:action => :update}) do |f| %>
        <% if @oferta_laborals.errors.any? %>
        <div class="margen alert alert-danger alert-dismissible fade show" role="alert">
          <button type="button" class="close" data-dismiss="alert" aria-label="Close">
          <span aria-hidden="true">&times;</span>
          </button>
          <% @oferta_laborals.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
          <% end %>
        </div>
        <% end %> 
          <div class="col-md-9 margen">
            <h3 class="text-left">
              <%= f.label :nombre_empresa %>
              <%= f.text_field :empresa, :class => "form-control form-control-lg" %>
            </h3>
            <br>
            <h3 class="text-left">
              <%= f.label :cargo_a_ofrecer %>
              <%= f.text_field :cargo, :class => "form-control form-control-lg" %>
            </h3>
            <h3 class="text-left">
              <%= f.label :descripción_de_la_oferta %>
              <%= f.trix_editor :descripcion, :class => "form-control form-control-lg" %>
            </h3>
            <br>
            <br>
            <div class="actions">
              <%= f.submit 'Guardar', :class => 'btn btn-outline-success btn-lg margen1' %>
              <br>
            </div>
            <%= link_to "VOLVER", inicio_path , :class => 'btn btn-outline-primary btn-lg margen1' %>
          <% end %>
        <% end %>
      </div>
      </div>
    </div>
 <!-- Fin Noticias -->
 <!-- Inicio Agenda -->
 <!-- Fin Agenda -->
 <!-- Inicio Limpiar flotante -->
 <div class="limpiar"></div>
 <!-- Fin Limpiar flotante -->
</main>
